#disable-coverage: true
#allow-incomplete-coverage: true
#allow-empty-coverage: true
coverage-report:
  - text
browser: true
timeout: 30
jobs: 8
include:
  - "src/test/js/spec/*.ts"
#exclude:
#  - "src/test/js/spec/ParserSpec.ts"
#  - "src/test/js/spec/ParserSpec2.ts"
snapshot-clean-cwd: true
color: true
reporter: base
